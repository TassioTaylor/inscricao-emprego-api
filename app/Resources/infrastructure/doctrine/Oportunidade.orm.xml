<?xml version="1.0" encoding="utf-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="Domain\model\Oportunidade">
        <id name="idOportunidade" type="integer">
            <generator strategy="IDENTITY"/>
        </id>

        <field name="descricao" type="string" column="descricao" />
        <field name="periodoinical" type="datetime" column="periodo_inicial" />
        <field name="periodofinal" type="datetime" column="periodo_final" />
        <field name="qtdVagas" type="integer" column="qtd_vagas" />

    </entity>
</doctrine-mapping>